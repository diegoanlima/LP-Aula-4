#include <stdio.h>

int main() {
    float nota;
    char conceito;

    printf("Digite a nota do aluno: ");
    scanf("%f", &nota);

   
    if (nota >= 9.0 && nota <= 10.0) {
        conceito = 'A';
    } else if (nota >= 7.5 && nota < 9.0) {
        conceito = 'B';
    } else if (nota >= 6.0 && nota < 7.5) {
        conceito = 'C';
    } else if (nota >= 4.0 && nota < 6.0) {
        conceito = 'D';
    } else if (nota >= 0.0 && nota < 4.0) {
        conceito = 'E';
    } else {
        conceito = 'I'; 
    }


    printf("O conceito do aluno Ã©: %c\n", conceito);

    return 0;
}
